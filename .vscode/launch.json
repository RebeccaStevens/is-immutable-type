{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "AVA Test Current Test File",
      "program": "${workspaceFolder}/node_modules/.pnpm/ava@5.2.0/node_modules/ava/entrypoints/cli.mjs",
      "args": ["${file}"],
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
