{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "AVA Test Current Test File",
      "program": "${workspaceFolder}/node_modules/.bin/ava",
      "args": ["${relativeFile}"],
      "outputCapture": "std",
      "skipFiles": ["<node_internals>/**/*.js"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
